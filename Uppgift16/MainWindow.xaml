<Window x:Class="Uppgift16.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift16"
        mc:Ignorable="d"
        Title="Inmatningskontroll" Height="450" Width="800">
    <Grid>
        <Label x:Name="yearLbl" Content="Ange födelseår" HorizontalAlignment="Left" Margin="206,149,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="yearBox" HorizontalAlignment="Left" Margin="206,175,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="okBtn" Content="Beräkna ålder" HorizontalAlignment="Left" Margin="206,202,0,0" VerticalAlignment="Top" Height="20" Width="98" Click="okBtn_Click"/>

    </Grid>
</Window>
