<Window x:Class="Uppgift14.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uppgift14"
        mc:Ignorable="d"
        Title="Medelvärde" Height="450" Width="800">
    <Grid>
        <Label x:Name="talLbl" Content="Mata in tal" HorizontalAlignment="Left" Margin="189,146,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="talBox" HorizontalAlignment="Left" Margin="189,172,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="okBtn" Content="Ok" HorizontalAlignment="Left" Margin="337,172,0,0" VerticalAlignment="Top" Height="29" Width="63" Click="okBtn_Click"/>
        <Label x:Name="midvalueLbl" Content="Totala medelvärdet" HorizontalAlignment="Left" Margin="194,252,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="midvalueBox" HorizontalAlignment="Left" Margin="194,283,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <ListBox x:Name="listBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="443,181,262,102"/>
        <Button x:Name="clearBtn" Content="Nollställ" HorizontalAlignment="Left" Margin="337,221,0,0" VerticalAlignment="Top" Height="29" Width="63" Click="clearBtn_Click"/>

    </Grid>
</Window>
